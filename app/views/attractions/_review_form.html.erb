<%#= simple_form_for([ @attraction, @review ], %>
<%= simple_form_for(review, url: attraction_reviews_path(@attraction),
                    data: {
                      insert_in_list_target: "form",
                      action: "submit->insert-in-list#send"
                    }) do |f| %>
  <%= f.input :content, as: :text, label: false %>
  <%= f.button :submit, class: 'btn btn-success' %>
<% end %>
