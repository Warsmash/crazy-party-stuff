<div class="book">
  <% @bookings.each do |booking|%>

  <div class="attracname"><%= booking.attraction.name %></div> /////
   <div class="date">
      <th><%= booking.start_date %></th> =>
      <th><%= booking.end_date %></th>
   </div>
  <% if booking.attraction.photo.attached? %>
    <%= cl_image_tag booking.attraction.photo.key, height: 300, width: 400, crop: :fill %>
  <% end %>

  <% end %>
</div>


